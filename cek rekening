  <script>
    const bgList = [
      'https://seocartel.sgp1.cdn.digitaloceanspaces.com/Computer-screen-code-glitch-animation-gif-background-free.gif',
      'https://seocartel.sgp1.cdn.digitaloceanspaces.com/nodes.gif',
      'https://seocartel.sgp1.cdn.digitaloceanspaces.com/9bc27292880429.5e569ff84e4d0.gif',
      'https://seocartel.sgp1.cdn.digitaloceanspaces.com/hacker-gif-7.gif'
    ];
    let bgIndex = 0;

    function gantiBackground() {
      bgIndex = (bgIndex + 1) % bgList.length;
      document.styleSheets[0].cssRules[0].style.backgroundImage = `url('${bgList[bgIndex]}')`;
    }

    async function cekRekening() {
      const bank = document.getElementById('bank').value;
      const nomor = document.getElementById('nomor').value.trim();
      const hasil = document.getElementById('hasil');

      hasil.innerHTML = `<div class="text-blue-300 font-medium">‚è≥ Sedang memproses...</div>`;

      try {
        const res = await fetch(`/api/checker?kode_bank=${bank}&nomor_rekening=${nomor}`);
        const data = await res.json();

        if (data.success) {
          hasil.innerHTML = `
            <div class="bg-green-700 text-green-100 p-3 rounded shadow-md">
              ‚úÖ <strong>Nama Pemilik:</strong> ${data.data.nama_pemilik}<br>
              <span class="text-sm">Bank: ${data.data.nama_bank}<br>Nomor: ${data.data.nomor_rekening}</span>
            </div>
          `;
        } else {
          hasil.innerHTML = `
            <div class="bg-red-700 text-red-100 p-3 rounded shadow-md">
              ‚ùå ${data.message}
            </div>
          `;
        }
      } catch (err) {
        hasil.innerHTML = `
          <div class="bg-yellow-600 text-yellow-100 p-3 rounded shadow-md">
            ‚ö†Ô∏è Gagal koneksi ke server.
          </div>
        `;
      }
    }
  </script>
